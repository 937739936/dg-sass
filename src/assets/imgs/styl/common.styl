.ellipsis
    overflow: hidden;
    letter-spacing:0;
    white-space: nowrap;
    max-width: 100%;
    width: auto;
    display: inline-block;
    text-overflow: ellipsis;
.tc
    text-align: center;
.tr
    text-align: right;
.fl
    float: left;
.fr
    float: right;
.cp
    cursor: pointer;
.mr8
    margin-right: 8px;
.mt8
    margin-top: 8px;
.mb8
    margin-bottom: 8px;
.mr16
    margin-right: 16px;
.mb16
    margin-bottom: 16px;
.mt16
    margin-top: 16px;
