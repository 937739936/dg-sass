.background-wrapper
    background-color: #f4f5f8;
    margin: -10px -20px;

.custom-main-content-inner
    flex:1;
    margin: -10px -30px -20px;
    // background-color: #f4f5f8;
    .page-title
        display:flex;
        align-items:center;
        justify-content:space-between;
        padding: 16px 30px;
        font-size: 16px;
        color: #141517;
        line-height: 24px;
        position: relative;
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
        background-color:#fff;

    .page-title:before
        position: absolute;
        content: '';
        width: 2px;
        height: 15px;
        left: 20px;
        top:50%;
        transform:translateY(-50%);
        background: #0053db;
