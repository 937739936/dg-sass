a:focus{
    text-decoration none
}
.s-eye
    position absolute
    right 12px
    top 0
    color rgba(0,0,0,0.30)
.s-title
    flex-row(space-between, center)
    height: 60px
    border-bottom: 1px solid #D8D8D8
    p
        flex-row(flex-start, center)
        height: 100%
        img
            width: 10px
            height: 16px
            margin-right: 14px
    span
        font-size: 18px
        font-weight: 500
.s-form-items
    flex-row(flex-start, center)
    width: 100%
    margin: 32px 0
    &>div
        flex-row(flex-start, center)
        flex: 1
        label
            text-align right
            padding-right 30px
        &>i
            margin-left: 10px
        &>label
            min-width: 120px
        &>p
            width: 258px
            flex-row(space-between, center)
            .ant-input-affix-wrapper,
            .ant-calendar-picker-input,
            .ant-input:not(textarea),
            .ant-select,
            .ant-select-selection,
            .ant-calendar-picker-input,
            .ant-calendar-picker,
            .ant-btn,
            .ant-select-selection__rendered
            .ant-calendar-picker>div
                width: 100% !important
                // height: 40px
            &>i
                margin: 0 2px
            .ant-select + .ant-select,
            .ant-btn
                margin-left: 10px
        .single
            width: 600px
            .ant-select
                margin-right: 10px

.title
    font-size 16px
    font-family PingFangSC-Medium
    text-align: left
.des
    font-family PingFangSC-Regular
    font-size 12px
    text-align: left

.s-list
    width: 100%
    color: rgba(0,0,0,0.65)
    font-family: PingFangSC-Regular
    font-size: 14px
    .ant-table-wrapper
        a
            display inline-block
            margin-right 8px
        a:last-child
            margin-right 0
    .s-list-header
        height: 54px
        flex-row(flex-start, center)
        background: #FAFAFA
        border-bottom: 1px solid rgba(0,0,0,0.09)
        li
            // width: 200px
            flex-row(flex-start, center)
            margin-left: 15px
            font-size 13px
    .s-list-info
        width: 100%
        flex-row(space-between, center)
        height: 105px
        li
            margin-left: 15px
            height: 100%
            flex-column(center, flex-start)
            .red
                color: #F5222D
        .status
            flex-row(flex-start, center)
            .wait
                width: 6px
                height: 6px
                border-radius: 6px
                background: #F5222D
                margin-right: 8px
            .success
                width: 6px
                height: 6px
                border-radius: 6px
                background: #52C41A
                margin-right: 8px
        li:first-child
            h3
                font-family: PingFangSC-Medium
                font-size: 18px
                color: #333333
                line-height: 22px
                margin-bottom: 10px
        .s-btn
            width: 300px
            flex-row(center, center)
            a
                // width: 70px
                //flex: 1
                color $mainColor
                padding: 0 10px
                line-height: 22px
                font-size: 14px
                text-align center
.no-data
    width: 100%
    height: 100px
    flex-row(center, center)
    font-size: 24px
    color: #999999
/* 分页边距 */
.s-pagination
    width: 100%
    flex-row(center, center)
    margin: 50px 0

/* 自定义卡片 */
.s-card
    width: 100%
.s-card-title
    flex-row(flex-start, center)
    font-size: 18px
    color: #565656
    font-family: PingFang-SC-Medium
    border-left: 2px solid $mainColor
    padding-left: 8px
    margin: 20px 0 20px 16px;
    line-height: 22px
.s-card-content
    width: 100%
    padding:14px 16px 32px 16px
    background: #fff
.divider
    background: #f4f5f8;
    height: 1px;
    margin-top:20px;

/* 提示 */
.s-tips
    flex-row(space-between, center)
    background: #fffbe6
    height: 40px
    border: 1px solid #FFE58F
    border-radius: 4px
    padding: 0 20px
    span
     font-size: 14px
     color:rgba(0,0,0,0.65)

.font-14
    font-size 14px
.font-12
    font-size 12px

